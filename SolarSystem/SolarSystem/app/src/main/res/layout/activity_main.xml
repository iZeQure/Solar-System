<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/jupiterText"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jorden"
        app:layout_constraintEnd_toEndOf="@+id/jorden"
        app:layout_constraintStart_toStartOf="@+id/jorden"
        app:layout_constraintTop_toBottomOf="@+id/jorden" />

    <ImageView
        android:id="@+id/jorden"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView14"
        app:layout_constraintStart_toEndOf="@+id/venus"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jorden" />

    <ImageView
        android:id="@+id/venus"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/jorden"
        app:layout_constraintStart_toEndOf="@+id/Mercury"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/venus" />

    <Switch
        android:id="@+id/venusSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/venusNavn"
        app:layout_constraintHorizontal_bias="0.791"
        app:layout_constraintStart_toStartOf="@+id/venusNavn"
        app:layout_constraintTop_toBottomOf="@+id/venusNavn" />

    <TextView
        android:id="@+id/merkurImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Merkur"
        app:layout_constraintEnd_toEndOf="@+id/Mercury"
        app:layout_constraintStart_toStartOf="@+id/Mercury"
        app:layout_constraintTop_toBottomOf="@+id/Mercury" />

    <ImageView
        android:id="@+id/Mercury"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/venus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/merkur" />

    <Switch
        android:id="@+id/merkurShitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/merkurImage"
        app:layout_constraintHorizontal_bias="0.791"
        app:layout_constraintStart_toStartOf="@+id/merkurImage"
        app:layout_constraintTop_toBottomOf="@+id/merkurImage" />

    <TextView
        android:id="@+id/venusNavn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Venus"
        app:layout_constraintEnd_toEndOf="@+id/venus"
        app:layout_constraintStart_toStartOf="@+id/venus"
        app:layout_constraintTop_toBottomOf="@+id/venus" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/jupiterImage"
        app:layout_constraintStart_toEndOf="@+id/jorden"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mars" />

    <TextView
        android:id="@+id/marsImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mars"
        app:layout_constraintEnd_toEndOf="@+id/imageView14"
        app:layout_constraintStart_toEndOf="@+id/jorden"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <Switch
        android:id="@+id/marsSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/marsImage"
        app:layout_constraintStart_toStartOf="@+id/marsImage"
        app:layout_constraintTop_toBottomOf="@+id/marsImage" />

    <ImageView
        android:id="@+id/jupiterImage"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView19"
        app:layout_constraintStart_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jupiter" />

    <TextView
        android:id="@+id/jupiterText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jupiter"
        app:layout_constraintEnd_toEndOf="@+id/jupiterImage"
        app:layout_constraintStart_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toBottomOf="@+id/jupiterImage" />

    <Switch
        android:id="@+id/jupiterSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/jupiterText"
        app:layout_constraintStart_toStartOf="@+id/jupiterText"
        app:layout_constraintTop_toBottomOf="@+id/jupiterText" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView20"
        app:layout_constraintStart_toEndOf="@+id/jupiterImage"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/saturn" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Saturn"
        app:layout_constraintEnd_toEndOf="@+id/imageView19"
        app:layout_constraintStart_toEndOf="@+id/jupiterImage"
        app:layout_constraintTop_toBottomOf="@+id/imageView19" />

    <Switch
        android:id="@+id/saturnSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/uranus" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Uranus"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintStart_toEndOf="@+id/imageView19"
        app:layout_constraintTop_toBottomOf="@+id/imageView20" />

    <Switch
        android:id="@+id/uranusSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <ImageView
        android:id="@+id/neptune"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marsSwitch"
        app:srcCompat="@drawable/neptun" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neptune"
        app:layout_constraintEnd_toEndOf="@+id/neptune"
        app:layout_constraintStart_toStartOf="@+id/neptune"
        app:layout_constraintTop_toBottomOf="@+id/neptune" />

    <Switch
        android:id="@+id/neptuneSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Switch
        android:id="@+id/jordenSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/textView13"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/neptune"
        app:layout_constraintTop_toBottomOf="@+id/jupiterSwitch"
        app:srcCompat="@drawable/pluto" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pluto"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintStart_toEndOf="@+id/neptune"
        app:layout_constraintTop_toBottomOf="@+id/imageView22" />

    <Switch
        android:id="@+id/plutoSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

</androidx.constraintlayout.widget.ConstraintLayout>